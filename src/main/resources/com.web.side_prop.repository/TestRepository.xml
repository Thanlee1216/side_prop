<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.side_prop.repository.TestRepository">

    <select id="testInfo" parameterType="hashmap" resultType="hashmap">
        /* TestDAO testInfo */
        select * from testTable
    </select>

    <insert id="testInsert" parameterType="hashmap">
        /* TestDAO testInsert */
        insert into testTable value(#{id}, #{name})
    </insert>
</mapper>
